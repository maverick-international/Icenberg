{
    "name": "mvrk/icenberg",
    "type": "library",
    "description": "Library to abstract away Advanced Custom Fields flexible content markup",
    "license": "MIT",
    "keywords": [
        "acf",
        "advanced custom fields",
        "wordpress",
        "flexible content"
    ],
    "authors": [
        {
            "name": "Daniel Devine",
            "email": "dan.devine@maverick-intl.com"
        },
        {
            "name": "Cathal Toomey",
            "email": "cathal.toomey@maverick-intl.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "MVRK\\Icenberg\\": "src/"
        },
        "files": [
            "cli/register-cli.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MVRK\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "symfony/var-dumper": "^5.1"
    }
}